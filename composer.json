{
    "name": "gilek/ewus",
    "description": "Biblioteka do usprawnienia komunikacji z systemem eWUŚ.",
    "keywords": ["ewus", "nfz", "biblioteka"],
    "homepage": "https://github.com/gilek/Ewus",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Kłak",
            "email": "klak.maciej@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gilek\\Ewus\\": "src"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gilek\\Ewus\\Test\\": "tests"
        }
    },
    "require": {
        "ext-soap": "*",
        "ext-dom": "*",
        "sabre/xml": "^2.2"
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^3.4",
        "phpstan/phpstan": "^0.12.58",
        "squizlabs/php_codesniffer": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
